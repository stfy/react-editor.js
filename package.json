    "lint": "eslint src",
    "test": "NODE_ENV=test mocha",
    "test:watch": "NODE_ENV=test mocha --watch",
    "prepublishOnly": "npm run lint && npm run clean && npm run build"
  },
  "keywords": [
    "react",
    "editor.js"
  ],
  "repository": {
    "type": "git",
