  "description": "React wrapper component for Editor.js",
  "main": "dist/react-editorjs.cjs.js",
  "module": "dist/react-editorjs.esm.js",
  "browser": "dist/react-editorjs.umd.js",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "cross-env NODE_ENV=development webpack-dev-server example/index.js --mode development",
